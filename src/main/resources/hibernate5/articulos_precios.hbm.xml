<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.cajaregistradora.struts6.models">

    <class name="ArticuloPrecio" schema="cajaregistradora01web" table="articulos_precios">
        <id name="codigo" type="long" column="codigo"></id>
        <property name="precio" type="integer" column="precio"/>
        <property name="fecha" type="date" column="fecha"/>
        <property name="hora" type="timestamp" column="hora"/>
    </class>

</hibernate-mapping>